<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    xmlns:fancy="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/app_bar_nutrition_screen"
    tools:context="com.example.mahesh.boltfitnesstracker.NutritionScreen">

    <com.github.lzyzsd.circleprogress.DonutProgress
        android:layout_marginTop="30dp"
        android:id="@+id/donut_progress"
        android:layout_width="175dp"
        android:layout_height="175dp"
        custom:donut_progress="75"
        android:layout_centerHorizontal="true"
        custom:donut_finished_color = "@color/colorPrimary"
        custom:donut_finished_stroke_width = "15dp"
        custom:donut_unfinished_stroke_width = "15dp"
        custom:donut_text= "kcal"
        custom:donut_text_size= "20dp"
        custom:donut_text_color= "@color/colorPrimary"



        />


    <com.github.lzyzsd.circleprogress.CircleProgress
        android:id="@+id/circle_progress"
        android:layout_marginTop="300dp"
        android:layout_centerHorizontal="true"

        android:layout_width="150dp"
        android:layout_height="150dp"
        custom:circle_progress="40"/>


</RelativeLayout>
